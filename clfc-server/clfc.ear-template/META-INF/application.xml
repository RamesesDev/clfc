<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://java.sun.com/xml/ns/javaee"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_5.xsd"
             version="5">


    <display-name>CLFC APPLICATION</display-name>

	<module>
		<java>app-libs.jar</java>
	</module>
	<module>
		<ejb>ruleserver.jar</ejb>
	</module>
	<module>
		<ejb>rameses-server.jar</ejb>
	</module>
	<module>
		<ejb>rameses-task-scheduler.jar</ejb>
	</module>
	<module>
		<java>services.jar</java>
	</module>
	<module>
		<web>
			<web-uri>http-invoker-server.war</web-uri>
			<context-root>clfc</context-root>
		</web>
	</module>
	<module>
		<web>
			<web-uri>clfc-branch.war</web-uri>
			<context-root>clfc-branch</context-root>
		</web>
	</module>
	
</application>
