<schema>
    <element name="user" tablename="user">
        <field name="objid" primary="true" />
        <field name="username" />
        <field name="lastname" />
        <field name="firstname" />
        <field name="middlename" />
        <field name="email" />
        <complex name="info" serializer="default" merge="true"/>
    </element>
</schema>
