import com.rameses.annotations.*;
import com.rameses.invoker.client.*;
import com.rameses.util.*;


class LoanOtherlendingService
{
	@PersistenceContext('java:loan')
	def em;

	@Service('DateService')
	def datesvc;

	@Service('UserService')
	def userSvc;
	
	@Env
    def env;
	
	@ProxyMethod
	def getPaymentTypeList(){
		return em.sqlContext.createQuery('select * from loan_otherlending_paymenttype').resultList;
	}
}	
